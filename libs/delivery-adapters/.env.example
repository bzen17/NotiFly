# Delivery Adapters - local .env
# NOTE: This file is for local development. Do NOT commit real secrets to git.

# SendGrid configuration - set your real key when sending real emails
# If you have a key, set it here; otherwise leave blank for mock mode.
SENDGRID_API_KEY=REDACTED_SENDGRID_API_KEY
SENDGRID_FROM=no-reply@example.com

# AWS SES (if using SES via delivery-adapters)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=REDACTED
AWS_SECRET_ACCESS_KEY=REDACTED
SES_FROM=no-reply@example.com

# Twilio (SMS)
TWILIO_ACCOUNT_SID=REDACTED
TWILIO_AUTH_TOKEN=REDACTED
TWILIO_FROM=+1500REDACTED

# FCM (push)
FCM_SERVER_KEY=REDACTED