# Delivery Adapters service environment
# Set the port the service listens on (default used by code: 3010)
DELIVERY_ADAPTERS_PORT=3010

# When true, adapter uses mock implementations instead of real providers
USE_MOCK=true

# SendGrid configuration - replace with your real values in a local .env (do NOT commit)
SENDGRID_API_KEY=YOUR_SENDGRID_API_KEY_HERE
SENDGRID_FROM=admin@example.com
